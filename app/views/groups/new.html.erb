<h1>Groups#new</h1>
<p>Find me in app/views/groups/new.html.erb</p>
<%= simple_form_for @group do |f| %>
  <%= f.input :name %>
  <%= f.button :submit, class: "btn-primary" %>
  <%= f.button :button, "Cancel", type: "reset", class: "btn-outline-secondary" %>
<% end %>
<%= link_to "groups", groups_path, class: "btn-outline-secondary"  %>
